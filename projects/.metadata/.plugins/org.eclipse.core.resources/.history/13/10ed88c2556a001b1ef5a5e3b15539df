	@Override
	public void AddBi(Integer bimodel) throws SQLException {
		


		String sii ="INSERT * From bicyclestoreschema.'bicycles' WHERE bimodel  =? ";
		
		
		try (Connection connection = Connector.get_connection())
		{	
				
		PreparedStatement preparedStatemnet = connection.prepareStatement(sii);
		ResultSet rs = preparedStatemnet.executeQuery();
				
			while(rs.next())
			{
				Employee employee = new Employee();
				//bicycles.setBiavailable(rs.getBoolean("biavailable"));
				employee.setBimodel.rs.getInteger("bimodel");
				//bicycles.setBiname(rs.getString("biname"));
				
				
			//user.setUserID(rs.getInt("id"));

			
		}
		
		
	}
	
	

}

